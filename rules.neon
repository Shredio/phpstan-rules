parametersSchema:
	shredio: structure([
		getObjectVarsByReferenceStaticMethod: structure([
			class: string()
			method: string()
		])
	])

services:
	-
		class: Shredio\PhpstanRules\Rule\GetObjectVarsByReferenceRule
		arguments: [%shredio.getObjectVarsByReferenceStaticMethod.class%, %shredio.getObjectVarsByReferenceStaticMethod.method%]
		tags:
			- phpstan.rules.rule
